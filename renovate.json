{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackageNames": ["com.example.foo:tool-types"],
      "versioning": "regex:^\\d+-\\d+-\\d+_(?<patch>\\d+)$"
    },
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true,
      "gitLabAutomerge": true
    }
  ]
}
